@model DisplayRecoveryCodesViewModel
@{
    ViewData["Title"] = "Recovery Code";
}

<h1>@ViewData["Title"].</h1>
<p class="text-success">@ViewData["StatusMessage"]</p>

<div>
    <h4>This is your Recovery Code</h4>
    <p>Storage these Recovery Code to authenticate two-factor authentication</p>
    <hr />
    <dl class="dl-horizontal">
        <dt>Codes:</dt>
        @foreach (var code in Model.Codes)
        {
        <dd>
            <text>@code</text>
        </dd>
        }
    </dl>
</div>